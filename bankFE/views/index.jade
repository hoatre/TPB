                    //span.labels  Brand 3
                  //label.tcb-inline
                    //input#line41.tc.tc-warning(type='checkbox', checked='true', , name='Line41')
                    //span.labels  Contact Center
                // Channel Options END
                hr.separator
                h5 Time
                select#SlidingTimeCbo.form-control
            hr.separator
            .row
              .col-sm-6.col-xs-6.text-center
                a.tile-button.btn.btn-white(href='#')
                  .tile-content-wrapper
                    #TotalNoTran.tile-content.text-primary
                      | 475
                    small
                      | Total No. of Transactions
              .col-sm-6.col-xs-6.text-center
                a.tile-button.btn.btn-primary(href='#')
                  .tile-content-wrapper
                    #TotalAmount.tile-content
                      span $
                      | 270
                    small
                      | Total Amount
