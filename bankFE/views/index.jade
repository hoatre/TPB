  //            span.labels  #{transactionType.TransactionName} (Bot)
  // Recent activities
  .row
    if(TransactionTypes == null || TransactionTypes.length == 0)
      | Khong co du lieu
    else
      each transactionType, index in TransactionTypes
        .col-lg-4.col-sm-12
          // Recent Activities
          .portlet.no-border-bottom
            .portlet-heading.dark
              .portlet-title
                h4
                  i.fa.fa-list-ul
                  |  Top 5 #{transactionType.TransactionName}
              .portlet-widgets
                a(data-toggle='collapse', data-parent='#accordion', href='#topFive#{transactionType.TransactionCode}')
                  i.fa.fa-chevron-down
              .clearfix
            .panel-collapse.collapse.in(id ='topFive#{transactionType.TransactionCode}')
              .portlet-body.no-padding
                .tc-tabs.no-margin
                  .tab-content.no-padding.no-border-left.no-border-right.no-border-bottom
                    .tab-pane.active
                      ul.lists
                                             
          // End Recent Activities
        .col-lg-4.col-sm-12
          // Recent Activities
          .portlet.no-border-bottom
            .portlet-heading.dark
              .portlet-title
                h4
                  i.fa.fa-list-ul
                  |  Bottom 5 #{transactionType.TransactionName}
              .portlet-widgets
                a(data-toggle='collapse', data-parent='#accordion', href='#topFiveDown#{transactionType.TransactionCode}')
                  i.fa.fa-chevron-down
              .clearfix
            .panel-collapse.collapse.in(id='topFiveDown#{transactionType.TransactionCode}')
              .portlet-body.no-padding
                .tc-tabs.no-margin
                  .tab-content.no-padding.no-border-left.no-border-right.no-border-bottom
                    .tab-pane.active
                      ul.lists
                                       
      // End Recent Activities
