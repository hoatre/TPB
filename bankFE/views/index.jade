  //        | Khong co du lieu
  //      else
  //        each transactionType, index in TransactionTypes
  //          label.tcb-inline.col-sm-3.col-lg-2
  //            input.tc.tc-success(type='checkbox', checked='true', name='#Five{transactionType.TransactionName}', id='Five#{transactionType.TransactionCode}') 
  //            span.labels  #{transactionType.TransactionName} (Top)
  //          label.tcb-inline.col-sm-3.col-lg-2
