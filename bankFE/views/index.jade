extends layout

block content  
  // START YOUR CONTENT HERE
  .row
    .col-lg-12.col-sm-12
      // Server Info Charts .morris
      .portlet
        .portlet-heading.inverse
          .portlet-title
            h4
              i.fa.fa-line-chart
              |  Branches Statics
          .portlet-widgets
            a#daterange.tooltip-primary(href='javascript:;', data-placement='top', data-rel='tooltip', title='DateRangePicker')
              i.fa.fa-calendar
            span.divider
            a(data-toggle='collapse', data-parent='#accordion', href='#m-charts')
              i.fa.fa-chevron-down
          .clearfix
        #m-charts.panel-collapse.collapse.in
          .portlet-body
            .row
              .col-sm-9
                h4 Branches Statics
                //canvas#chart.chart-holder(style='height: 220px!important; min-height:220px;')
                #chartCD(style='height: 250px; width: 100%;')
              .col-sm-3
                h4 Settings
                //hr.separator
                //h5 Channels
                // Channel Options
                //.tcb
                  //label.tcb-inline
                    //input#line11.tc.tc-success(type='checkbox', checked='true', name='Line11')
