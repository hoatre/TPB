    script(src='/assets/js/plugins/bootstrap-datepicker/bootstrap-datepicker.js')
    script(src='/assets/js/plugins/jquery-sparkline/jquery.sparkline.min.js')
    script(src='/assets/js/plugins/easypiechart/jquery.easypiechart.min.js')
