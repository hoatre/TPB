doctype html
html(lang='en')
 include ../layouts/header.jade
 body
  #wrapper
   #main-container
    //top navigation
    include ../layouts/topnav.jade
    //left menu
    include ../layouts/leftmenu.jade
    //main content
    // BEGIN MAIN PAGE CONTENT
    #page-wrapper
      // BEGIN PAGE HEADING ROW
      .row
        .col-lg-12
          // BEGIN BREADCRUMB
          .breadcrumbs
            ul.breadcrumb
              li
                a(href='#') Home
              li.active Dashboard
            .b-right.hidden-xs
              ul
                li
                  a(href='#', title='')
                    i.fa.fa-signal
                li
                  a(href='#', title='')
                    i.fa.fa-comments
                li.dropdown
                  a(href='#', title='', data-toggle='dropdown')
                    i.fa.fa-plus
                    span  Tasks
                  ul.dropdown-menu.dropdown-primary.dropdown-menu-right
                    li
                      a(href='#') Add new task
                    li
                      a(href='#') Statement
                    li
                      a(href='#') Settings
          // END BREADCRUMBs          
          // /#ek-layout-button
          .qs-layout-menu
            #qs-setting-btn.btn.btn-gray.qs-setting-btn
              i.fa.fa-cog.bigger-150.icon-only
            #qs-setting-box.qs-setting-box
              .hidden-xs.hidden-sm
                span.bigger-120 Layout Options
                .hr.hr-dotted.hr-8
                label
                  input#fixed-navbar.tc(type='checkbox')
                  span.labels(id='#fixed-navbar')  Fixed NavBar
                label
                  input#fixed-sidebar.tc(type='checkbox')
                  span.labels(id='#fixed-sidebar')  Fixed NavBar+SideBar
                label
                  input#sidebar-toggle.tc(type='checkbox')
                  span.labels(id='#sidebar-toggle')  Sidebar Toggle
                label
                  input#in-container.tc(type='checkbox')
                  span.labels(id='#in-container')
                    | Inside
                    strong .container
                .space-4
              span.bigger-120 Color Options
              .hr.hr-dotted.hr-8
              label
                input#side-bar-color.tc(type='checkbox')
                span.labels(id='#side-bar-color')  SideBar (Light)
              ul
                li
                  button.btn(style='background-color:#d15050;', onclick="swapStyle('assets/css/themes/style.css')")
                li
                  button.btn(style='background-color:#86618f;', onclick="swapStyle('assets/css/themes/style-1.css')")
                li
                  button.btn(style='background-color:#ba5d32;', onclick="swapStyle('assets/css/themes/style-2.css')")
                li
                  button.btn(style='background-color:#488075;', onclick="swapStyle('assets/css/themes/style-3.css')")
                li
                  button.btn(style='background-color:#4e72c2;', onclick="swapStyle('assets/css/themes/style-4.css')")
          // /#ek-layout-button
        // /.col-lg-12
      // /.row
      // END PAGE HEADING ROW
      .row
        .col-lg-12
          block content 
      // BEGIN FOOTER CONTENT
      .footer
        .footer-inner
          // basics/footer
          .footer-content
            | Â© 2015
          // /basics/footer
      button#back-to-top.btn.btn-primary.btn-sm.back-to-top(type='button')
        i.fa.fa-angle-double-up.icon-only.bigger-110
      // END FOOTER CONTENT
    // /#page-wrapper
    // END MAIN PAGE CONTENT
  block footer    
    //footer
    // core JavaScript
    script(src='/assets/js/jquery.min.js')
    script(src='/assets/js/bootstrap.min.js')
