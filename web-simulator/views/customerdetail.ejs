	var arr=[];
	var totalrecord=0;
	var socket = io.connect('http://127.0.0.1:3002/');
	var objPage={value:'',start:0,end:50}
	jQuery(function($) {
		var customerID = $("#_CustomerID").val();
		//alert(customerID);
		
		socket.emit('customerID',customerID);
		
		socket.on('_customerDetail',function(data){
			document.getElementById('divCustomerTitle').innerHTML="<h2>"+data[0].FULLNAME+"</h2>";
			var sex = data[0].GENDER==1?"MALE":"FEMALE";
			var strDesscription='';
			strDesscription ="<dt>Birthday</dt>";
			strDesscription +="<dd>"+data[0].DOB+"</dt>"
			strDesscription +="<dt>Gender</dt>";
			strDesscription +="<dd>"+sex+"</dt>";
			strDesscription +="<dt>Address</dt>";
			strDesscription +="<dd>"+data[0].ADDRESS+"</dt>";
			
			document.getElementById('divCustomerDesscription').innerHTML = strDesscription;
			socket.emit('CUSTOMERLOGS_TOP5_TRANSACTION_SEND_MESSAGE',data[0].ACCOUNTNUMBER);
		});
		socket.on('CUSTOMERLOGS_TOP5_TRANSACTION_DATA', function(data){
			//alert(data[0].trx_id);
			var strTableTop5='';
			strTableTop5="<table class='table table-bordered table-hover tc-table'>";
			strTableTop5+="<thead><tr>";
			strTableTop5+="<th class='hidden-xs'>Time</th>";
			strTableTop5+="<th class='hidden-xs'>Activity</th>";
			strTableTop5+="<th class='hidden-xs'>Detail</th>";
			strTableTop5+="</tr></thead><tbody>";
			
			for(var i=0;i<data.length;i++)
			{

				var transname=data[i].trx_name==null?'':data[i].trx_name;
				var chanelname=data[i].ch_name==null?'':data[i].ch_name+", ";
				var chaneladd=data[i].ch_add==null?'':data[i].ch_add;
				var activity = "["+data[i].trx_id+"] "+transname+' ('+data[i].amount+' VND, '+data[i].acc_no+')';				
				var detail=chanelname+chaneladd;
				strTableTop5+="<tr><td class='hidden-xs'>"+jQuery.timeago(new Date(data[i].timestamp))+"</td>";
				
				strTableTop5+="<td class='hidden-xs'>"+activity+"</td>";
				strTableTop5+="<td class='hidden-xs'>"+detail+"</td></tr>";
			}
			strTableTop5+="</tbody></table>";
			//alert(strTableTop5);
			document.getElementById('divCustomerTop5').innerHTML=strTableTop5;
		});
	});

</script>

